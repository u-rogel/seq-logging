{
  "name": "seq-logging",
  "version": "1.1.2",
  "description": "Sends structured log events to the Seq HTTP ingestion API",
  "keywords": [
    "seq",
    "logging"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build:browser": "rollup --config ./rollup.browser.config.js",
    "build:node": "rollup --config ./rollup.node.config.js",
    "build": "npm run build:browser && npm run build:node",
    "coverage": "nyc --reporter=text mocha",
    "start": "node ./example/example.js",
    "typecheck": "tsc --noEmit"
  },
  "author": "Datalust and contributors",
  "license": "Apache-2.0",
  "homepage": "https://github.com/datalust/seq-logging",
  "repository": {
    "type": "git",
    "url": "https://github.com/datalust/seq-logging.git"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.2",
    "mocha": "^9.2.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nyc": "^15.1.0",
    "rollup": "^3.25.3",
    "simple-mock": "^0.8.0",
    "superagent": "^6.1.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  }
}
